	journal = {Annals Of Glaciology, Vol 32, 2001},
	pages = {7–13},
	se = {ANNALS OF GLACIOLOGY},
	sn = {0260-3055},
	timestamp = {2007.07.06},
	title = {Snow texture: a comparison of empirical versus simulated texture index for Alpine snow},
	ut = {ISIP:000171277900002},
	volume = {32},
	year = {2001}
}

