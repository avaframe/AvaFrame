	se = {ANNALS OF GLACIOLOGY},
	sn = {0260-3055},
	timestamp = {2007.07.06},
	title = {Atmospheric and hydrological boundary conditions for slushflow initiation due to snowmelt},
	ut = {ISIP:000076481500069},
	volume = {26},
	year = {1998}
}

