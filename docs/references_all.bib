@article{FuVi1998,
	abstract = {We present regression models to estimate maximum avalanche run-out
	distances in the western Catalan Pyrenees (northeast Spain). The
	regression equations have been calculated from topographic parameters
	of 216 well-known avalanche paths, mapped on avalanche location maps
	at a scale of 1:50 000. Historical information about avalanche maximum
	run-out zones has been available for some avalanche paths but most
