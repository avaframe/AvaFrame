@article{TaMa2001,
	abstract = {Dielectric measurements of snow were carried out in the temperature
	range -15 degrees to 0 degreesC and in the frequency range 50 Hz
	to 5 MHz. The snow samples (about 400 kg m(-3) density) used were
	stored snow (average particle size: 2 mm) and hoar-frost (particle
	size: <1 to 5 mm). The frequency characteristics of dielectric parameter's
	showed a dielectric dispersion (Davidson-Cole type) around 30 kHz
	and a low-frequency dielectric dispersion (Cole-Cole circular law
	type). The a.c. conductivity showed a dielectric dispersion around
	30 kHz and two characteristic constant values in the frequency ranges
	above I MHz and below 100 Hz (the high-frequency conductivity sigma
	and the low-frequency conductivity sigma (LOW)). The low-frequency
	conductivity sigma (LOW) showed a C we peak at about -2 degreesC.
	This behavior has never been noted by previous researchers. The sigma
	(LOW) showed an activation energy of about 1 eV below -5 degreesC.
	This means that the sigma (LOW) is mainly caused by a surface conduction.
	The activation energy increased with increasing temperature above
	-5 degreesC. This means that the sigma (LOW) in this temperature
	range is affected by the quasi-liquid layer on ice surfaces. The
	sigma (LOW) above -2 degreesC decreased with increasing temperature.
	The apparently curious behavior near the melting temperature is attributed
	to the numerous free ice surfaces within the porous snow. This conclusion
	was reached because our measurements without the free ice surfaces
	showed no such conductivity peaks for solid polycrystalline ice samples
	and for snow samples soaked with kerosene in the cooling process.},
	author = {I. Takei and N. Maeno},
	bn = {0-946417-27-X},
	cl = {INNSBRUCK, AUSTRIA},
	ct = {International Symposium on Snow, Avalanches and Impact of the ForestEOLEOLCover},
	cy = {MAY 22-26, 2000},
	ed = {Hutter, K},
	journal = {Annals Of Glaciology, Vol 32, 2001},
	pages = {14–18},
	se = {ANNALS OF GLACIOLOGY},
	sn = {0260-3055},
	timestamp = {2007.07.06},
	title = {The low-frequency conductivity of snow near the melting temperature},
	ut = {ISIP:000171277900003},
	volume = {32},
	year = {2001}
}

@article{TaKoSaIzKo2001,
	abstract = {Snowdrifting processes and the wind-velocity profiles around a collector
	and a blower snow fence were investigated in a cold wind tunnel.
	The purpose was to ascertain the effect of wind direction on drift
	control by snow fences. Three different cases were studied for both
	types of snow fence, and the resultant snowdrifts were compared.
	In the first case, the snow fence was perpendicular to the wind direction.
	In the second and third cases, it was tilted by 30 degrees and 45
	degrees. When the collector snow fence was tilted, the amounts of
	snowdrift were much less than when the fence was perpendicular to
	the wind direction, because the area with low wind velocity was reduced
	to half behind the tilted fence. On the other hand, the blowing effect
	of the blower snow fence increased when it was set up at an angle
	to the wind direction. It is necessary to investigate the position
	where the blown snow is deposited by the tilted blower snow fence.},
	author = {Y. Takeuchi and S. Kobayashi and T. Sato and K. Izumi and K. Kosugi and W. Xin and J. P. Zhang and Y. H. Peng},
