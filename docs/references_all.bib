@ARTICLE{AnMeRi2003,
  author = {C. Ancey and M. Meunier and D. Richard},
  title = {Inverse problem in avalanche dynamics models},
  journal = {Water Resources Research},
  year = {2003},
  volume = {39},
  pages = {1099},
  number = {4},
  abstract = {Avalanche dynamics models are increasingly used to estimate the features
	of extremely rare events for avalanche zoning. They employ a frictional
	coefficient, which reflects something close to snow viscosity. As
	this coefficient is more conceptual than physical, it cannot be measured
