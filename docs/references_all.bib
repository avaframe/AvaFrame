	ct = {International Symposium on Snow and Avalanches},
	cy = {MAY 26-30, 1997},
	ed = {McClung, DM},
	file = {DeBuScLoAd1998.pdf},
	journal = {Annals Of Glaciology, Vol 26, 1998},
	pages = {247–252},
	se = {ANNALS OF GLACIOLOGY},
	sn = {0260-3055},
	timestamp = {2007.07.06},
	title = {Density, velocity and friction measurements in a dry-snow avalanche},
	ut = {ISIP:000076481500047},
	volume = {26},
	year = {1998}
}

