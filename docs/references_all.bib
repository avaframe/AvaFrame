	journal = {Annals Of Glaciology, Vol 26, 1998},
	pages = {19–21},
	se = {ANNALS OF GLACIOLOGY},
	sn = {0260-3055},
	timestamp = {2007.07.06},
	title = {Snow-creep forces on masts},
	ut = {ISIP:000076481500004},
	volume = {26},
	year = {1998}
}

