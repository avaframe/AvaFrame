@ARTICLE{HaAk1998,
  author = {A. Hachikubo and E. Akitaya},
  title = {Daytime preservation of surface-hoar crystals},
  journal = {Annals Of Glaciology, Vol 26, 1998},
  year = {1998},
  volume = {26},
  pages = {22--26},
  abstract = {Surface hear growing for several clear and humid days were observed.
	During daytime, air and snow-surface temperature increased and relative
	humidity decreased, hence evaporation (sublimation) occurred at the
	snow surface. The amount of evaporation calculated using a bulk-transfer
	method suggests that the surface-hear crystals which grew during
	the previous night should have disappeared but they were observed
	to survive on the snow surface even during the daytime. During the
	following night, new surface-hear crystals formed on top of the older
	ones and grew even larger. This result indicates that, although the
	surface-hear crystals evaporated into the air during the daytime,
	snow grains beneath the surface were warmed by solar radiation and
	evaporated to the air. They may partially condense into the surface-hear
	crystals and make up for the reduction in size. Depth-hear crystals
	formed beneath the snow surface for several days and the surface
	layer, composed of both types of hear crystal, showed a very weak
