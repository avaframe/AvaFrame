@article{ScIs2001,
	abstract = {In winter 1998/99, high-frequency pressure measurements with 10 cm
	sensors mounted 1-19 in above ground were carried out in the upper
	run-out zone of the avalanche test site at Vallee de la Sionne, Switzerland.
