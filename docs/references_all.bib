	pages = {331–338},
	pg = {8},
	pi = {CAMBRIDGE},
	publisher = {Int Glaciological Soc},
	rp = {Lehning, M, SLF, Swiss Fed Inst Snow \& Avalanche Res, WSL, FluelastrEOLEOL11, Davos, CH-7260 Switzerland.},
	sc = {Geosciences, Multidisciplinary},
