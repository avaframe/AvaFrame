	pages = {153–158},
	se = {ANNALS OF GLACIOLOGY},
	sn = {0260-3055},
	timestamp = {2007.07.06},
	title = {Measurements and one-dimensional model calculations of snow transport over a mountain ridge},
	ut = {ISIP:000171277900026},
	volume = {32},
