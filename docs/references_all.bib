	journal = {Annals Of Glaciology, Vol 26, 1998},
	pages = {231–236},
	se = {ANNALS OF GLACIOLOGY},
	sn = {0260-3055},
	timestamp = {2007.07.06},
	title = {Passive snow removal with a vortex generator at the Pegasus runway, Antarctica},
	ut = {ISIP:000076481500044},
	volume = {26},
	year = {1998}
}

